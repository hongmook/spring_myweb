<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <!-- 인터페이스 이름(com.khm.mapper.MemberMapper)과 xml의 namespace 속성값이 일치 -->
  <mapper namespace="com.khm.mapper.MemberMapper">
  	<!-- id="인터페이스 메소드 이름" -->
  	<select id="getById" resultType="com.khm.dto.Member">
  		select * from member where id = #{id}
  	</select>

  </mapper>
